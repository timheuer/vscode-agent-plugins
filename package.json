{
  "name": "vscode-agent-plugins",
  "displayName": "Agent Plugins Browser",
  "description": "A browser of agent marketplaces to install plugins.",
  "version": "0.0.0-placeholder",
  "preview": true,
  "author": {
    "name": "Tim Heuer",
    "url": "https://github.com/timheuer"
  },
  "publisher": "timheuer",
  "icon": "icon.png",
  "repository": {
    "type": "git",
    "url": "https://github.com/timheuer/vscode-agent-plugins"
  },
  "bugs": {
    "url": "https://github.com/timheuer/vscode-agent-plugins/issues"
  },
  "qna": "https://github.com/timheuer/vscode-agent-plugins/issues",
  "engines": {
    "vscode": "^1.109.0"
  },
  "categories": [
    "Other"
  ],
  "extensionDependencies": [
    "vscode.github-authentication"
  ],
  "activationEvents": [
  ],
  "main": "./dist/extension.js",
  "contributes": {
    "viewsContainers": {
      "activitybar": [
        {
          "id": "agent-plugins",
          "title": "Agent Plugins",
          "icon": "icon.png"
        }
      ]
    },
    "views": {
      "agent-plugins": [
        {
          "id": "vscode-agent-plugins.marketplaceExplorer",
          "name": "Marketplace",
          "icon": "icon.png",
          "contextualTitle": "Agent Plugins Marketplace"
        }
      ]
    },
    "commands": [
      {
        "command": "vscode-agent-plugins.browseMarketplace",
        "title": "Agent Plugins: Browse Marketplace"
      },
      {
        "command": "vscode-agent-plugins.addMarketplaceUrl",
        "title": "Agent Plugins: Add Marketplace URL",
        "icon": "$(add)"
      },
      {
        "command": "vscode-agent-plugins.removeMarketplaceUrl",
        "title": "Agent Plugins: Remove Marketplace URL"
      },
      {
        "command": "vscode-agent-plugins.refreshTreeView",
        "title": "Agent Plugins: Refresh",
        "icon": "$(refresh)"
      },
      {
        "command": "vscode-agent-plugins.installPlugin",
        "title": "Install Plugin",
        "icon": "$(cloud-download)"
      },
      {
        "command": "vscode-agent-plugins.previewItem",
        "title": "Preview Item"
      },
      {
        "command": "vscode-agent-plugins.collapseNode",
        "title": "Collapse",
        "icon": "$(collapse-all)"
      },
      {
        "command": "vscode-agent-plugins.openSettings",
        "title": "Agent Plugins: Settings",
        "icon": "$(gear)"
      },
      {
        "command": "vscode-agent-plugins.gitHubSignIn",
        "title": "Agent Plugins: Sign In to GitHub",
        "icon": "$(github)"
      },
      {
        "command": "vscode-agent-plugins.gitHubAuthStatus",
        "title": "Agent Plugins: GitHub Auth Status"
      },
      {
        "command": "vscode-agent-plugins.openMarketplaceRepo",
        "title": "Open Repository",
        "icon": "$(github)"
      }
    ],
    "menus": {
      "view/title": [
        {
          "command": "vscode-agent-plugins.refreshTreeView",
          "when": "view == vscode-agent-plugins.marketplaceExplorer",
          "group": "navigation"
        },
        {
          "command": "vscode-agent-plugins.addMarketplaceUrl",
          "when": "view == vscode-agent-plugins.marketplaceExplorer",
          "group": "navigation"
        },
        {
          "command": "vscode-agent-plugins.openSettings",
          "when": "view == vscode-agent-plugins.marketplaceExplorer",
          "group": "navigation"
        },
        {
          "command": "vscode-agent-plugins.gitHubSignIn",
          "when": "view == vscode-agent-plugins.marketplaceExplorer",
          "group": "navigation"
        },
        {
          "command": "vscode-agent-plugins.browseMarketplace",
          "when": "view == vscode-agent-plugins.marketplaceExplorer"
        }
      ],
      "view/item/context": [
        {
          "command": "vscode-agent-plugins.openMarketplaceRepo",
          "when": "view == vscode-agent-plugins.marketplaceExplorer && viewItem == marketplace-github",
          "group": "inline@1"
        },
        {
          "command": "vscode-agent-plugins.installPlugin",
          "when": "view == vscode-agent-plugins.marketplaceExplorer && viewItem == plugin",
          "group": "inline"
        },
        {
          "command": "vscode-agent-plugins.collapseNode",
          "when": "view == vscode-agent-plugins.marketplaceExplorer && viewItem =~ /marketplace|plugin|group/",
          "group": "inline@99"
        },
        {
          "command": "vscode-agent-plugins.installPlugin",
          "when": "view == vscode-agent-plugins.marketplaceExplorer && viewItem == plugin",
          "group": "1_install"
        },
        {
          "command": "vscode-agent-plugins.collapseNode",
          "when": "view == vscode-agent-plugins.marketplaceExplorer && viewItem =~ /marketplace|plugin|group/",
          "group": "2_navigation"
        }
      ]
    },
    "configuration": {
      "title": "Agent Plugins",
      "properties": {
        "agentPlugins.marketplaces": {
          "type": "array",
          "default": [ ],
          "description": "List of marketplace.json URLs to browse for agent plugins.",
          "items": {
            "type": "string",
            "pattern": "^https?://"
          }
        },
        "agentPlugins.logLevel": {
          "type": "string",
          "enum": [
            "off",
            "error",
            "warn",
            "info",
            "debug",
            "trace"
          ],
          "default": "info",
          "description": "Log level for extension output channel logging."
        }
      }
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run package",
    "compile": "npm run check-types && npm run lint && node esbuild.js",
    "watch": "npm-run-all -p watch:*",
    "watch:esbuild": "node esbuild.js --watch",
    "watch:tsc": "tsc --noEmit --watch --project tsconfig.json",
    "package": "npm run check-types && npm run lint && node esbuild.js --production",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "npm run compile-tests && npm run compile && npm run lint",
    "check-types": "tsc --noEmit",
    "lint": "eslint src",
    "test": "vscode-test",
    "deploy": "vsce publish"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.10",
    "@types/node": "20.x",
    "@types/vscode": "^1.109.0",
    "@typescript-eslint/eslint-plugin": "^8.31.1",
    "@typescript-eslint/parser": "^8.31.1",
    "@vscode/test-cli": "^0.0.11",
    "@vscode/test-electron": "^2.5.2",
    "esbuild": "^0.25.3",
    "eslint": "^9.25.1",
    "npm-run-all": "^4.1.5",
    "typescript": "^5.8.3"
  },
  "dependencies": {
    "@timheuer/vscode-ext-logger": "^0.1.35",
    "@vscode-elements/elements": "^2.5.0",
    "@vscode/codicons": "^0.0.44"
  }
}
